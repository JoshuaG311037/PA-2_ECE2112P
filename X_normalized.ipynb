    "\n",
    "\n",
    "# Random 5x5 ndarray\n",
    "X = np.random.rand(5, 5)\n",
    "\n",
    "# Normalize the ndarray\n",
    "normalized_X = (X - X.mean()) / X.std()\n",
    "\n",
    "# Original / normalized\n",
    "print(f\"Orig:\\n {X}\\n\")\n",
    "print(f\"Norm:\\n {normalized_X}\")"
   ]
